package Uno;
import javax.swing.*;
public class Factorial {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
	Long resultado = 1L;
	int numero=Integer.parseInt(JOptionPane.showInputDialog( "introduce nuemro"));
	
	for(int i=numero; i>0; i--) {
		resultado=resultado*i;
	}

	System.out.println( "el factorial " + numero + "es " + resultado) ;
	}

}
